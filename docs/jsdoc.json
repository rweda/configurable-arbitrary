{
  "source": {
    "include": "./",
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|docs)"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "opts": {
    "template": "node_modules/docdash",
    "destination": "public/docs/",
    "private": true,
    "readme": "README.md"
  }
}
