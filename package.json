  "bugs": {
    "url": "https://github.com/rweda/configurable-arbitrary/issues"
  },
  "homepage": "https://github.com/rweda/configurable-arbitrary#readme",
  "optionalDependencies": {
    "jsverify": "^0.8.2"
  },
  "dependencies": {
    "lodash.merge": "^4.6.0",
    "lodash.pickby": "^4.6.0"
  },
  "devDependencies": {
    "ava": "^0.20.0",
    "ava-verify": "0.0.1",
    "docdash": "^0.4.0",
    "jsdoc": "^3.5.0",
    "jsverify": "^0.8.2",
    "mockery": "^2.1.0",
    "nyc": "^11.0.3",
    "proxyquire": "^1.8.0",
    "rewire": "^2.5.2",
