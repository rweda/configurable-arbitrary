{
  "name": "configurable-arbitrary",
  "version": "0.0.1",
  "description": "QuickCheck Modules",
  "main": "ConfigurableArbitrary.js",
  "scripts": {
    "test": "NODE_PATH=. nyc --reporter html --reporter text ava",
    "docs": "jsdoc -c docs/jsdoc.json -r"
  },
  "ava": {
    "files": [
      "test/**/*.js",
      "examples/**/test/**/*.js"
